                <SearchField width="500px" class="outerBorder" visible="true" search=".onSearchListOrder2" showSearchButton="true" id="IdSearchHome" />
                <Button icon="sap-icon://person-placeholder" text="Sign In" type="Transparent" press="onHomeCustomerSignIn" />
                <Button icon="sap-icon://create" text="Register" type="Transparent" press="onHomeRegister" />
                <Button icon="sap-icon://cart" text="Cart" class="homeButtonToolbar" type="Transparent" press="onHomeCart" />
            </tnt:ToolHeader>

