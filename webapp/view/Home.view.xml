            <VBox alignContent="Center" alignItems="Center" justifyContent="Center">
                <Label text="Category" class="catagoryFont" />
            </VBox>

            <GenericTile backgroundImage="/../images/fashion.jpg" class="tileCatagory" header="Fashion" subheader="" press="onFashion" />

            <GenericTile backgroundImage="/../images/appl2png.png" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Electronic" subheader="" press="onElectronics" />

            <GenericTile backgroundImage="/../images/mobile.jpg" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Mobile" subheader="" mode="HeaderMode" press="press" />

            <GenericTile backgroundImage="/../images/g2.png" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Grocery" subheader="" press="press" />

            <GenericTile backgroundImage="/../images/Furniture.jpg" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Furniture" subheader="" press="press" />

            <GenericTile backgroundImage="/../images/Beauty.jpg" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Beauty Product" subheader="" press="press" />

            <GenericTile backgroundImage="/../images/tt2.jpg" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Toys" subheader="" mode="HeaderMode" press="press" />

            <GenericTile backgroundImage="/../images/veg1.png" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Vegitable" subheader="" press="press" />

            <VBox alignContent="Center" alignItems="Center" justifyContent="Center">
                <Label text="Top offers on brands" class="brandFont" />
            </VBox>

            <VBox class="vBoxMargin" direction="Row">
                <GenericTile backgroundImage="/../images/nike3.png" class="vBoxItemMargin" header="" subheader="" press="press" />

                <GenericTile backgroundImage="/../images/zara2.png" class="vBoxItemMargin" header="" subheader="" mode="HeaderMode" press="press" />

                <GenericTile backgroundImage="/../images/puma2.png" class="vBoxItemMargin" header="" subheader="" mode="HeaderMode" press="press" />

                <GenericTile backgroundImage="/../images/reebok3.png" class="vBoxItemMargin" header="" subheader="" press="press" />
            </VBox>

            <VBox alignContent="Center" alignItems="Center" justifyContent="Center">
                <Label text="Bank Offer" class="brandFont" />

                <SlideTile class="bankOfferSlide" width="1100px" displayTime="5000">
                    <GenericTile backgroundImage="/../images/bank2slide.jpg" press="pressOnTileOne" width="1100px" />
                </SlideTile>
            </VBox>

            <VBox alignContent="Center" alignItems="Center" justifyContent="Center">
                <Label text="Extra Benefits" class="brandFont" />
            </VBox>

            <SlideTile class="marginGif" width="1000px" displayTime="5000">
                <GenericTile backgroundImage="/../images/slide1.gif" press="pressOnTileOne" width="1000px" />
            </SlideTile>
            <SlideTile class="slideMargin" width="460px" displayTime="5000">
                <GenericTile backgroundImage="/../images/ss3.jpg" press="pressOnTileOne" width="500px" />
            </SlideTile>
